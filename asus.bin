@echo off
cls

AFUWINx64.exe BIOS.rom /o
AFUWINx64.exe BIOS.rom /p

for /L %%i in (1,1,4) do (
    for /f "delims=" %%j in ('call randstr.bat 4') do (
        for /f "delims=" %%k in ('call randstr.bat 4') do (
            set "volume%%i=%%j-%%k"
        )
    )
)

Volumeid64.exe C: !volume1! /accepteula
Volumeid64.exe D: !volume2! /accepteula
Volumeid64.exe E: !volume3! /accepteula
Volumeid64.exe F: !volume4! /accepteula

netsh winsock reset catalog
netsh int ip reset
netsh advfirewall reset
netsh int reset all
netsh int ipv4 reset
netsh int ipv6 reset
WMIC PATH WIN32_NETWORKADAPTER WHERE PHYSICALADAPTER=TRUE CALL DISABLE >nul 2>&1
WMIC PATH WIN32_NETWORKADAPTER WHERE PHYSICALADAPTER=TRUE CALL ENABLE >nul 2>&1

net stop winmgmt /y
net start winmgmt
