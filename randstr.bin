@echo off
setlocal enabledelayedexpansion

if "%1"=="" (
    echo Usage: %~nx0 [length]
    echo Example: %~nx0 4
    goto :eof
)

set "length=%1"

set "chars=0123456789ABCDEF"
set "randomStr="

for /L %%i in (1,1,%length%) do (
    set /a "index=!random! %% 16"
    set "randomStr=!randomStr!!chars:~!index!,1!"
)

echo !randomStr!
exit /b